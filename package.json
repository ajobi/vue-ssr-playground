{
  "name": "vue-ssr-playground",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Andrej Bilec <andrej.bilec@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:content": "node ./src/content/server.js",
    "start:header": "node ./src/header/server.js",
    "start": "concurrently \"yarn start:content\" \"yarn start:header\"",
    "build:content:client": "webpack --config ./src/content/webpack.config.js ",
    "build:content:server": "webpack --config ./src/content/webpack.server.config.js",
    "build:content": "yarn build:content:client && yarn build:content:server",
    "build:header:client": "webpack --config ./src/header/webpack.config.js ",
    "build:header:server": "webpack --config ./src/header/webpack.server.config.js",
    "build:header": "yarn build:header:client && yarn build:header:server",
    "build": "yarn build:header && yarn build:content",
    "express": "node server.js"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "vue": "^2.5.11",
    "vue-server-renderer": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
